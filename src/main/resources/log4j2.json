{
	"configuration": {
		"name": "CoolbenchLoggerConfiguration",
		"packages": "io.tolgadurak.coolbench.cli",
		"appenders": {
			"Console": {
				"name": "STDOUT",
				"PatternLayout": {
					"pattern": "%msg%n"
				},
				"ThresholdFilter": {
					"level": "debug"
				}
			}
		},
		"loggers": {
			"root": {
				"level": "INFO",
				"AppenderRef": {
					"ref": "STDOUT"
				}
			}
		}
	}
}